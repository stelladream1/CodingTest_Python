def factorial_iterative(n):
    result = 1
    for i in range(1,n+1):
        result = result * i
    return result

def factorial_recursiven(n):
    if n <= 1:
        return 1
    return n * factorial_recursiven(n-1)

print("반복적으로 구현: ",factorial_recursiven(5))
print("재귀적으로 구현: ",factorial_iterative(5))
